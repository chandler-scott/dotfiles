global !p
import os

def find_csproj_file():
    current_file_dir = os.path.dirname(vim.current.buffer.name)
    for file in os.listdir(current_file_dir):
        if file.endswith(".csproj"):
            return file.strip(".csproj")
    return "No .csproj file found"
endglobal

